      }
    },
    "android": {
      "package": "com.gbiondi.nossopoint",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "7e480f65-cc6b-476f-b57d-270e50206802"
      }
    },
    "owner": "gbiondinova"
  
  }
